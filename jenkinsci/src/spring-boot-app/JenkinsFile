pipeline {
    agent {
        docker {
            image 'maven:latest'
        }
    }
    stages { 
            stage('maven-test') {
                steps {
                    sh 'mvn -version'
                }
            }

            stage('gitcheckout') {
                git branch: 'main', url: 'https://github.com/subbu4565/jenkins-docker.git'
                sh 'echo success'
            }
    }
}